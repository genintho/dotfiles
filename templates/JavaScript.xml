<templateSet group="JavaScript">
  <template name="react-perf" value="import Perf from 'react-addons-perf';&#10;window.perf = Perf;&#10;" description="Insert React Performance" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JS_EXPRESSION" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="debug" value="debugger;" description="Insert debugger" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="clog" value="console.log($END$);" description="console.log" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="CoffeeScript" value="true" />
    </context>
  </template>
  <template name="ctrace" value="console.trace($END$);" description="console.trace" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="CoffeeScript" value="true" />
    </context>
  </template>
  <template name="imp" value="import $END$ from '$FILE_NAME$';" description="insert import" toReformat="false" toShortenFQNames="true">
    <variable name="FILE_NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="imp-shallow" value="import PureShallowComponent from '$END$/components/PureShallowComponent';" description="Impore the Shallow component" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="import_lodash" value="import _ $END$ from &quot;lodash&quot;;" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="import_react" value="import React, {&#10;    PropTypes,&#10;} from 'react';&#10;$END$" description="Import React and PropTypes" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="import_track" value="import trackEvent from '$END$/hotelRedux/util/trackEvent';" description="Import the event tracker" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
</templateSet>